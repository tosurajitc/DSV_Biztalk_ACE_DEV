<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" 
    xmlns:xmi="http://www.omg.org/XMI" 
    xmlns:ComIbmCompute.msgnode="ComIbmCompute.msgnode"
    xmlns:ComIbmWSInput.msgnode="ComIbmWSInput.msgnode"
    xmlns:ComIbmWSReply.msgnode="ComIbmWSReply.msgnode"
    xmlns:ComIbmRoute.msgnode="ComIbmRoute.msgnode"
    xmlns:ComIbmLabel.msgnode="ComIbmLabel.msgnode"
    xmlns:ComIbmRouteToLabel.msgnode="ComIbmRouteToLabel.msgnode"
    xmlns:ComIbmSOAPRequest.msgnode="ComIbmSOAPRequest.msgnode" 
    xmlns:ComIbmXslMqsi.msgnode="ComIbmXslMqsi.msgnode" 
    xmlns:ComIbmFileInput.msgnode="ComIbmFileInput.msgnode"
    xmlns:ComIbmFileOutput.msgnode="ComIbmFileOutput.msgnode"
    xmlns:ComIbmHTTPRequest.msgnode="ComIbmHTTPRequest.msgnode"
    xmlns:ComIbmMQInput.msgnode="ComIbmMQInput.msgnode"
    xmlns:ComIbmMQOutput.msgnode="ComIbmMQOutput.msgnode"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" 
    xmlns:eflow="http://www.ibm.com/wbi/2005/eflow" 
    xmlns:epis_common_flows_lib_MQInput.subflow="epis/common/flows/lib/MQInput.subflow" 
    xmlns:epis_common_flows_lib_MQOutput.subflow="epis/common/flows/lib/MQOutput.subflow"
    xmlns:epis_compression_lib_GZipCompressAndB64EncodeElement.subflow="epis/compression/lib/GZipCompressAndB64EncodeElement.subflow" 
    xmlns:epis_enrichment_lib_EPIS_MessageEnrichment.subflow="epis/enrichment/lib/EPIS_MessageEnrichment.subflow"
    xmlns:epis_encoding_lib_Base64Encoder.subflow="epis/encoding/lib/Base64Encoder.subflow"
    xmlns:epis_storage_lib_BlobStorage.subflow="epis/storage/lib/BlobStorage.subflow"
    xmlns:utility="http://www.ibm.com/wbi/2005/eflow_utility" 
    nsURI="{FLOW_NAME}.msgflow" 
    nsPrefix="{FLOW_NAME}.msgflow">

  <eClassifiers xmi:type="eflow:FCMComposite" name="FCMComposite_1" nodeLayoutStyle="RECTANGLE">
    <eSuperTypes href="http://www.ibm.com/wbi/2005/eflow#//FCMBlock"/>
    
    <!-- ================================================== -->
    <!-- TEMPLATE PROPERTIES - COMPLETE SET -->
    <!-- ================================================== -->
    
    <!-- HTTP INPUT PROPERTIES -->
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.HTTP_URL_SPECIFIER" name="HTTP_URL_SPECIFIER" defaultValueLiteral="{HTTP_URL_SPECIFIER}">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>
    
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.HTTP_METHOD" name="HTTP_METHOD" defaultValueLiteral="POST">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>
    
    <!-- MQ INPUT PROPERTIES -->
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.INPUT_QUEUE" name="INPUT_QUEUE" lowerBound="1" defaultValueLiteral="{INPUT_QUEUE_NAME}">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>
    
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.INPUT_MSG_PARSING" name="INPUT_MSG_PARSING" defaultValueLiteral="XMLNSC">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>
    
    <!-- FILE INPUT PROPERTIES -->
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.FILE_INPUT_DIRECTORY" name="FILE_INPUT_DIRECTORY" defaultValueLiteral="/var/mqsi/input">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>
    
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.FILE_INPUT_PATTERN" name="FILE_INPUT_PATTERN" defaultValueLiteral="*.xml">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>
    
    <!-- COMPRESSION PROPERTIES -->
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.COMPRESS_MSG_SIZE" name="COMPRESS_MSG_SIZE" lowerBound="1" defaultValueLiteral="103809024">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eStructuralFeatures>
    
    <!-- BLOB STORAGE PROPERTIES -->
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.BLOB_STORAGE_ACCOUNT" name="BLOB_STORAGE_ACCOUNT" defaultValueLiteral="{BLOB_STORAGE_ACCOUNT}">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>
    
    <!-- XSL TRANSFORM PROPERTIES -->
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.stylesheetName" name="stylesheetName" defaultValueLiteral="{FLOW_NAME}.xsl">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>
    
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.stylesheetName.internal_npcompiler" name="stylesheetName.internal_npcompiler">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>
    
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.IS_XSLTRANSFORM" name="IS_XSLTRANSFORM" lowerBound="1" defaultValueLiteral="true">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    </eStructuralFeatures>
    
    <!-- BEFORE ENRICHMENT PROPERTIES -->
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.IS_BEFORE_ENRICHMENT" name="IS_BEFORE_ENRICHMENT" lowerBound="1" defaultValueLiteral="true">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    </eStructuralFeatures>
    
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.BEFORE_INPUT_DIR" name="BEFORE_INPUT_DIR" defaultValueLiteral="{APP_NAME}/{FLOW_NAME}/enrichment">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>
    
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.BEFORE_FILE_NAME" name="BEFORE_FILE_NAME" defaultValueLiteral="BeforeEnrichmentConf.json">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>

    <!-- AFTER ENRICHMENT PROPERTIES -->
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.IS_AFTER_ENRICHMENT" name="IS_AFTER_ENRICHMENT" lowerBound="1" defaultValueLiteral="false">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    </eStructuralFeatures>
    
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.AFTER_INPUT_DIR" name="AFTER_INPUT_DIR" defaultValueLiteral="{APP_NAME}/{FLOW_NAME}/enrichment">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>
    
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.AFTER_FILE_NAME" name="AFTER_FILE_NAME" defaultValueLiteral="AfterEnrichmentConf.json">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>

    <!-- SOAP PROPERTIES -->
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.SOAP_SERVICE_URL" name="SOAP_SERVICE_URL" defaultValueLiteral="{SOAP_SERVICE_URL}">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>

    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.WSDL_FILE_NAME" name="WSDL_FILE_NAME" defaultValueLiteral="{WSDL_FILE_NAME}">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>

    <!-- HTTP REQUEST PROPERTIES -->
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.HTTP_REQUEST_URL" name="HTTP_REQUEST_URL" defaultValueLiteral="{HTTP_REQUEST_URL}">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>

    <!-- OUTPUT PROPERTIES -->
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.OUTPUT_QUEUE" name="OUTPUT_QUEUE" defaultValueLiteral="{OUTPUT_QUEUE_NAME}">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>

    <!-- FILE OUTPUT PROPERTIES -->
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.FILE_OUTPUT_DIRECTORY" name="FILE_OUTPUT_DIRECTORY" defaultValueLiteral="/var/mqsi/output">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>
    
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.FILE_OUTPUT_PATTERN" name="FILE_OUTPUT_PATTERN" defaultValueLiteral="output_%d%t.xml">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>

    <!-- ROUTING PROPERTIES -->
    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.HAS_METHOD_ROUTING" name="HAS_METHOD_ROUTING" defaultValueLiteral="false">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    </eStructuralFeatures>

    <eStructuralFeatures xmi:type="ecore:EAttribute" xmi:id="Property.ROUTING_METHODS" name="ROUTING_METHODS" defaultValueLiteral="method1,method2,method3">
      <eType xmi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eStructuralFeatures>

    <!-- Branding and Graphics -->
    <translation xmi:type="utility:TranslatableString" key="{FLOW_NAME}" bundleName="{FLOW_NAME}" pluginId="{APP_NAME}_Developer"/>
    <colorGraphic16 xmi:type="utility:GIFFileGraphic" resourceName="platform:/plugin/{APP_NAME}_Developer/icons/full/obj16/{FLOW_NAME}.gif"/>
    <colorGraphic32 xmi:type="utility:GIFFileGraphic" resourceName="platform:/plugin/{APP_NAME}_Developer/icons/full/obj30/{FLOW_NAME}.gif"/>
    
    <composition>
      
      <!-- ================================================== -->
      <!-- SECTION 1: INPUT NODES (Dynamic - one of three) -->
      <!-- ================================================== -->
      {DYNAMIC_INPUT_SECTION}
      <!--
      Possible inputs:
      - HTTP Input (ComIbmWSInput)
      - MQ Input (epis_common_flows_lib_MQInput.subflow)
      - File Input (ComIbmFileInput)
      -->
      
      <!-- ================================================== -->
      <!-- SECTION 2: EVENT PROCESSING NODES -->
      <!-- ================================================== -->
      
      <!-- 1. INPUT EVENT MESSAGE - ALWAYS REQUIRED -->
      <nodes xmi:type="ComIbmCompute.msgnode:FCMComposite_1" 
            xmi:id="FCMComposite_1_2" 
            location="-221,338" 
            rotation="RIGHT_TO_LEFT" 
            computeExpression="esql://routine/{FLOW_NAME}#{FLOW_NAME}_InputEventMessage.Main">
        <translation xmi:type="utility:ConstantString" string="InputEventMessage"/>
      </nodes>

      <!-- ================================================== -->
      <!-- SECTION 3: BLOB STORAGE NODES (Conditional) -->
      <!-- ================================================== -->
      {DYNAMIC_BLOB_STORAGE}
      <!--
      BlobStorage subflow - Only if has_blob_storage=True
      -->

      <!-- ================================================== -->
      <!-- SECTION 4: ENRICHMENT NODES (Conditional) -->
      <!-- ================================================== -->
      
      {DYNAMIC_ENRICHMENT_SUBFLOW}
      <!--
      BeforeEnrichment subflow - Only if has_before_enrichment=True
      -->
      
      <!-- ================================================== -->
      <!-- SECTION 5: MAIN COMPUTE NODE (Always required) -->
      <!-- ================================================== -->
      
      <!-- 2. MAIN COMPUTE - ALWAYS REQUIRED -->
      <nodes xmi:type="ComIbmCompute.msgnode:FCMComposite_1" 
            xmi:id="FCMComposite_1_1" 
            location="376,68" 
            computeExpression="esql://routine/{FLOW_NAME}#{FLOW_NAME}_Compute.Main">
        <translation xmi:type="utility:ConstantString" string="Compute"/>
      </nodes>
      
      <!-- ================================================== -->
      <!-- SECTION 6: ROUTING NODES (Conditional) -->
      <!-- ================================================== -->
      {DYNAMIC_ROUTING_SECTION}
      <!--
      Route, Label, RouteToLabel nodes - Only if has_method_routing=True
      -->
      
      <!-- ================================================== -->
      <!-- SECTION 7: TRANSFORM NODES (Conditional) -->
      <!-- ================================================== -->
      
      {DYNAMIC_XSL_TRANSFORM}
      <!--
      XSL Transform node - Only if has_xsl_transform=True
      -->
      
      <!-- 3. AFTER ENRICHMENT - ALWAYS REQUIRED -->
      <nodes xmi:type="ComIbmCompute.msgnode:FCMComposite_1" 
            xmi:id="FCMComposite_1_12" 
            location="796,45" 
            computeExpression="esql://routine/{FLOW_NAME}#{FLOW_NAME}_AfterEnrichment.Main">
        <translation xmi:type="utility:ConstantString" string="AfterEnrichment"/>
      </nodes>
      
      <!-- ================================================== -->
      <!-- SECTION 8: ENCODING NODES (Conditional) -->
      <!-- ================================================== -->
      
      {DYNAMIC_BASE64_ENCODER}
      <!--
      Base64Encoder - Only if has_base64_encoding=True
      -->
      
      {DYNAMIC_GZIP_SUBFLOW}
      <!--
      GZip Compression - Only if has_gzip_compression=True
      -->
      
      <!-- ================================================== -->
      <!-- SECTION 9: SERVICE REQUEST NODES (Conditional) -->
      <!-- ================================================== -->
      
      {DYNAMIC_SOAP_NODE}
      <!--
      SOAP Request - Only if has_soap_request=True
      -->
      
      {DYNAMIC_HTTP_REQUEST}
      <!--
      HTTP Request - Only if has_http_request=True
      -->
      
      <!-- ================================================== -->
      <!-- SECTION 10: OUTPUT NODES (Conditional) -->
      <!-- ================================================== -->
      
      {DYNAMIC_OUTPUT_SUBFLOW}
      <!--
      MQOutput or FileOutput or HTTPReply - Based on output_type
      -->
      
      <!-- 4. OUTPUT EVENT MESSAGE - ALWAYS REQUIRED -->
      <nodes xmi:type="ComIbmCompute.msgnode:FCMComposite_1" 
            xmi:id="FCMComposite_1_13" 
            location="1000,300" 
            rotation="RIGHT_TO_LEFT" 
            computeExpression="esql://routine/{FLOW_NAME}#{FLOW_NAME}_OutputEventMessage.Main">
        <translation xmi:type="utility:ConstantString" string="OutputEventMessage"/>
      </nodes>

      <!-- 5. AFTER EVENT MESSAGE - ALWAYS REQUIRED -->
      <nodes xmi:type="ComIbmCompute.msgnode:FCMComposite_1" 
            xmi:id="FCMComposite_1_15" 
            location="1600,100" 
            computeExpression="esql://routine/{FLOW_NAME}#{FLOW_NAME}_AfterEventMsg.Main">
        <translation xmi:type="utility:ConstantString" string="AfterEventMessage"/>
      </nodes>

      <!-- 6. FAILURE HANDLER - ALWAYS REQUIRED -->
      <nodes xmi:type="ComIbmCompute.msgnode:FCMComposite_1" 
            xmi:id="FCMComposite_1_11" 
            location="1600,300" 
            computeExpression="esql://routine/{FLOW_NAME}#{FLOW_NAME}_Failure.Main">
        <translation xmi:type="utility:ConstantString" string="FailureHandler"/>
      </nodes>

      <!-- ================================================== -->
      <!-- SECTION 11: CONNECTIONS -->
      <!-- ================================================== -->
      {DYNAMIC_CONNECTIONS}
      
    </composition>

    <!-- ================================================== -->
    <!-- PROPERTY ORGANIZATION - COMPLETE & UNCHANGED -->
    <!-- ================================================== -->
    <propertyOrganizer>
      
      <!-- ============================================ -->
      <!-- HTTP INPUT PROPERTIES (Include if input_type=HTTP) -->
      <!-- ============================================ -->
      {IF_HTTP_INPUT_START}
      <propertyDescriptor groupName="Group.HTTP" configurable="true" describedAttribute="Property.HTTP_URL_SPECIFIER">
        <propertyName xmi:type="utility:TranslatableString" key="Property.HTTP_URL_SPECIFIER" bundleName="ComIbmWSInput" pluginId="com.ibm.etools.mft.ibmnodes.definitions"/>
        
        <propertyDescriptor groupName="Group.HTTP" configurable="true" describedAttribute="Property.HTTP_METHOD">
          <propertyName xmi:type="utility:TranslatableString" key="Property.HTTP_METHOD" bundleName="ComIbmWSInput" pluginId="com.ibm.etools.mft.ibmnodes.definitions"/>
          {NESTED_DESCRIPTORS}
        </propertyDescriptor>
      </propertyDescriptor>
      {IF_HTTP_INPUT_END}
      
      <!-- ============================================ -->
      <!-- MQ INPUT PROPERTIES (Include if input_type=MQ) -->
      <!-- ============================================ -->
      {IF_MQ_INPUT_START}
      <propertyDescriptor groupName="Group.COMPRESSION" configurable="true" userDefined="true" describedAttribute="Property.COMPRESS_MSG_SIZE">
        <propertyName xmi:type="utility:TranslatableString" key="Property.COMPRESS_MSG_SIZE" bundleName="epis/common/flows/lib/MQInput" pluginId="EPIS_CommonFlows_Lib"/>
        
        <propertyDescriptor groupName="Group.INPUT" propertyEditor="com.ibm.etools.mft.ibmnodes.editors.MRMessageDomainPropertyEditor" describedAttribute="Property.INPUT_MSG_PARSING">
          <propertyName xmi:type="utility:TranslatableString" key="Property.INPUT_MSG_PARSING" bundleName="epis/common/flows/lib/MQInput" pluginId="EPIS_CommonFlows_Lib"/>
          
          <propertyDescriptor groupName="Group.INPUT" propertyEditor="com.ibm.etools.mft.ibmnodes.editors.mq.MQQueueNameEditor" configurable="true" describedAttribute="Property.INPUT_QUEUE">
            <propertyName xmi:type="utility:TranslatableString" key="Property.INPUT_QUEUE" bundleName="epis/common/flows/lib/MQInput" pluginId="EPIS_CommonFlows_Lib"/>
            {NESTED_DESCRIPTORS}
          </propertyDescriptor>
        </propertyDescriptor>
      </propertyDescriptor>
      {IF_MQ_INPUT_END}
      
      <!-- ============================================ -->
      <!-- FILE INPUT PROPERTIES (Include if input_type=FILE) -->
      <!-- ============================================ -->
      {IF_FILE_INPUT_START}
      <propertyDescriptor groupName="Group.FILE_INPUT" propertyEditor="com.ibm.etools.mft.ibmnodes.editors.file.FileNodesBrowseDirPropertyEditor" configurable="true" describedAttribute="Property.FILE_INPUT_DIRECTORY">
        <propertyName xmi:type="utility:TranslatableString" key="Property.FILE_INPUT_DIRECTORY" bundleName="ComIbmFileInput" pluginId="com.ibm.etools.mft.ibmnodes.definitions"/>
        
        <propertyDescriptor groupName="Group.FILE_INPUT" propertyEditor="com.ibm.etools.mft.ibmnodes.editors.file.FileReadNodeFilePropertyEditor" configurable="true" describedAttribute="Property.FILE_INPUT_PATTERN">
          <propertyName xmi:type="utility:TranslatableString" key="Property.FILE_INPUT_PATTERN" bundleName="ComIbmFileInput" pluginId="com.ibm.etools.mft.ibmnodes.definitions"/>
          {NESTED_DESCRIPTORS}
        </propertyDescriptor>
      </propertyDescriptor>
      {IF_FILE_INPUT_END}
      
      <!-- ============================================ -->
      <!-- BLOB STORAGE PROPERTIES (Include if has_blob_storage=True) -->
      <!-- ============================================ -->
      {IF_BLOB_STORAGE_START}
      <propertyDescriptor groupName="Group.BLOB_STORAGE" configurable="true" describedAttribute="Property.BLOB_STORAGE_ACCOUNT">
        <propertyName xmi:type="utility:TranslatableString" key="Property.BLOB_STORAGE_ACCOUNT" bundleName="epis/storage/lib/BlobStorage" pluginId="EPIS_Storage_Lib"/>
        {NESTED_DESCRIPTORS}
      </propertyDescriptor>
      {IF_BLOB_STORAGE_END}
      
      <!-- ============================================ -->
      <!-- ENRICHMENT PROPERTIES (Include if has_enrichment=True) -->
      <!-- ============================================ -->
      {IF_ENRICHMENT_START}
      <propertyDescriptor groupName="Group.BEFORE_ENRICHMENT" propertyEditor="com.ibm.etools.mft.ibmnodes.editors.file.FileReadNodeFilePropertyEditor" configurable="true" describedAttribute="Property.BEFORE_FILE_NAME">
        <propertyName xmi:type="utility:TranslatableString" key="Property.BEFORE_FILE_NAME" bundleName="ComIbmFileRead" pluginId="com.ibm.etools.mft.ibmnodes.definitions"/>
        
        <propertyDescriptor groupName="Group.BEFORE_ENRICHMENT" propertyEditor="com.ibm.etools.mft.ibmnodes.editors.file.FileNodesBrowseDirPropertyEditor" configurable="true" describedAttribute="Property.BEFORE_INPUT_DIR">
          <propertyName xmi:type="utility:TranslatableString" key="Property.BEFORE_INPUT_DIR" bundleName="ComIbmFileRead" pluginId="com.ibm.etools.mft.ibmnodes.definitions"/>
          
          <propertyDescriptor groupName="Group.AFTER_ENRICHMENT" propertyEditor="com.ibm.etools.mft.ibmnodes.editors.file.FileReadNodeFilePropertyEditor" configurable="true" describedAttribute="Property.AFTER_FILE_NAME">
            <propertyName xmi:type="utility:TranslatableString" key="Property.AFTER_FILE_NAME" bundleName="ComIbmFileRead" pluginId="com.ibm.etools.mft.ibmnodes.definitions"/>
            
            <propertyDescriptor groupName="Group.AFTER_ENRICHMENT" propertyEditor="com.ibm.etools.mft.ibmnodes.editors.file.FileNodesBrowseDirPropertyEditor" configurable="true" describedAttribute="Property.AFTER_INPUT_DIR">
              <propertyName xmi:type="utility:TranslatableString" key="Property.AFTER_INPUT_DIR" bundleName="ComIbmFileRead" pluginId="com.ibm.etools.mft.ibmnodes.definitions"/>
              {NESTED_DESCRIPTORS}
            </propertyDescriptor>
          </propertyDescriptor>
        </propertyDescriptor>
      </propertyDescriptor>
      {IF_ENRICHMENT_END}
      
      <!-- ============================================ -->
      <!-- XSL TRANSFORM PROPERTIES (Include if has_xsl_transform=True) -->
      <!-- ============================================ -->
      {IF_XSL_START}
      <propertyDescriptor groupName="Group.XSL" propertyEditor="com.ibm.etools.mft.ibmnodes.editors.XSLNamePropertyEditor" compiler="com.ibm.etools.mft.ibmnodes.compilers.XSLTNamePropertyCompiler" configurable="true" describedAttribute="Property.stylesheetName">
        <propertyName xmi:type="utility:TranslatableString" key="Property.stylesheetName" bundleName="ComIbmXslMqsi" pluginId="com.ibm.etools.mft.ibmnodes.definitions"/>
        
        <propertyDescriptor groupName="Group.InternalRuntimeGroup" readOnly="true" hidden="true" userDefined="true" describedAttribute="Property.stylesheetName.internal_npcompiler">
          <propertyName xmi:type="utility:TranslatableString" key="Property.stylesheetName.internal_npcompiler" bundleName="{FLOW_NAME}" pluginId="{APP_NAME}_Developer"/>
          {NESTED_DESCRIPTORS}
        </propertyDescriptor>
      </propertyDescriptor>
      {IF_XSL_END}
      
      <!-- ============================================ -->
      <!-- SOAP PROPERTIES (Include if has_soap_request=True) -->
      <!-- ============================================ -->
      {IF_SOAP_START}
      <propertyDescriptor groupName="Group.SOAP" configurable="true" describedAttribute="Property.SOAP_SERVICE_URL">
        <propertyName xmi:type="utility:TranslatableString" key="Property.SOAP_SERVICE_URL" bundleName="ComIbmSOAPRequest" pluginId="com.ibm.etools.mft.ibmnodes.definitions"/>
        
        <propertyDescriptor groupName="Group.SOAP" propertyEditor="com.ibm.etools.mft.ibmnodes.editors.file.FileReadNodeFilePropertyEditor" configurable="true" describedAttribute="Property.WSDL_FILE_NAME">
          <propertyName xmi:type="utility:TranslatableString" key="Property.WSDL_FILE_NAME" bundleName="ComIbmSOAPRequest" pluginId="com.ibm.etools.mft.ibmnodes.definitions"/>
          {NESTED_DESCRIPTORS}
        </propertyDescriptor>
      </propertyDescriptor>
      {IF_SOAP_END}
      
      <!-- ============================================ -->
      <!-- HTTP REQUEST PROPERTIES (Include if has_http_request=True) -->
      <!-- ============================================ -->
      {IF_HTTP_REQUEST_START}
      <propertyDescriptor groupName="Group.HTTP_REQUEST" configurable="true" describedAttribute="Property.HTTP_REQUEST_URL">
        <propertyName xmi:type="utility:TranslatableString" key="Property.HTTP_REQUEST_URL" bundleName="ComIbmHTTPRequest" pluginId="com.ibm.etools.mft.ibmnodes.definitions"/>
        {NESTED_DESCRIPTORS}
      </propertyDescriptor>
      {IF_HTTP_REQUEST_END}
      
      <!-- ============================================ -->
      <!-- OUTPUT PROPERTIES (Always present) -->
      <!-- ============================================ -->
      {IF_MQ_OUTPUT_START}
