{
  "extraction_metadata": {
    "document_id": "${document_id}",
    "document_name": "${document_name}",
    "extraction_timestamp": "${timestamp}",
    "extraction_version": "${version}"
  },
  "flow_structure": {
    "is_multi_flow": "${boolean}",
    "flow_count": "${number}",
    "pattern_type": "${pattern_type}"
  },
  "message_flows": [
    {
      "flow_name": "${flow_name}",
      "app_name": "${app_name}",
      "server_name": "${server_name}",
      "flow_type": "${flow_type}",
      "is_sat_flow": "${boolean}",
      "is_synchronous": "${boolean}",
      
      "input_configuration": {
        "input_type": "${input_type}",
        "input_node_name": "${node_name}",
        "input_details": {
          "uri_pattern": "${uri_pattern}",
          "http_method": "${http_method}",
          "content_type": "${content_type}",
          "queue_name": "${queue_name}",
          "file_pattern": "${file_pattern}"
        }
      },
      
      "nodes": [
        {
          "node_id": "${node_id}",
          "node_name": "${node_name}",
          "node_type": "${node_type}",
          "is_entry_point": "${boolean}",
          "node_purpose": "${purpose_description}"
        }
      ],
      
      "node_connections": [
        {
          "source_node_id": "${source_node_id}",
          "target_node_id": "${target_node_id}",
          "terminal_type": "${terminal_type}",
          "filter_condition": "${filter_condition}"
        }
      ],
      
      "transformation_components": {
        "has_enrichment": "${boolean}",
        "has_xsl_transform": "${boolean}",
        "has_soap_request": "${boolean}",
        "xsl_files": [
          "${xsl_file_name}"
        ]
      },
      
      "technical_description": "${technical_description}"
    }
  ],
  
  "connected_systems": [
    {
      "system_name": "${system_name}",
      "system_id": "${system_id}",
      "system_type": "${system_type}",
      "service_endpoints": [
        {
          "environment": "${environment}",
          "uri": "${uri}",
          "port": "${port}"
        }
      ]
    }
  ],
  
  "routing_configuration": {
    "requires_routing": "${boolean}",
    "routing_methods": [
      "${method_name}"
    ],
    "subscribing_endpoints": [
      {
        "endpoint": "${endpoint_name}",
        "filter_criteria": "${filter_criteria}"
      }
    ]
  },
  
  "performance_requirements": {
    "business_critical": "${boolean}",
    "operation_hours": "${hours}",
    "max_downtime": "${duration}",
    "time_sensitive": "${boolean}",
    "transaction_processing_time": "${duration}",
    "expected_daily_load": "${count}",
    "expected_peak_load": "${count}",
    "audit_events_count": "${count}",
    "avg_message_size": "${size}",
    "max_message_size": "${size}"
  },
  
  "biztalk_migration": {
    "biztalk_endpoints": [
      "${endpoint_name}"
    ],
    "publishers": [
      "${publisher_name}"
    ],
    "subscribers": [
      "${subscriber_name}"
    ]
  }
}