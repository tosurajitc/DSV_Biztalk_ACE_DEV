  "item": [
    {
      "name": "Test Customer Format to CDM Document Transformation",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/xml"
          },
          {
            "key": "Authorization",
            "value": "Basic QWxhZGprakdFRReGR5ZGF3"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "<CustomerFormat><Document>Example Document</Document></CustomerFormat>"
        },
        "url": {
          "raw": "https://ace-server-airandsea.cw1-generic.in-server.com/EPIS_CW1_IN_Document_App",
          "host": [
            "ace-server-airandsea"
          ],
          "path": [
            "cw1-generic",
            "in-server",
            "com",
            "EPIS_CW1_IN_Document_App"
          ]
        }
      },
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test('Customer format is transformed to CDM document', function () {",
              "  pm.expect(pm.response.text()).to.include('CDM Document');",
              "});"
            ]
          }
        }
      ]
    },
